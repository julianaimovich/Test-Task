1. SELECT FPHONENUMBER FROM PHONE_NUMBERS WHERE USERID IS NULL;

2. SELECT FPHONENUMBER FROM PHONE_NUMBERS 
   JOIN USERS ON PHONE_NUMBERS.USERID = USERS.USERID 
   WHERE DATE_CREATE > '28-FEB-19 11.59.59 PM';
   
3. SELECT COUNT(FPHONENUMBER), USERID FROM PHONE_NUMBERS
   WHERE USERID IS NOT NULL
   GROUP BY USERID;
   
4. SELECT FPHONENUMBER FROM PHONE_NUMBERS
   WHERE FPHONEAREA = 650 AND USERID IS NOT NULL;
   
5. ALTER TABLE USERS
   ALTER COLUMN DATE_CREATE YEAR(4);
   
6. UPDATE PHONE_NUMBERS SET USERID = NULL
   WHERE FPHONEAREA = 303;
   
7. INSERT INTO PHONE_NUMBERS (FPHONEAREA, FPHONENUMBER) VALUES
   (800, 441001821991),
   (800, 445141821984);
